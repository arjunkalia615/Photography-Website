      ]
    },
    {
      "source": "/:path*\\.jpeg",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=2592000, immutable" }
      ]
    },
    {
      "source": "/:path*\\.png",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=2592000, immutable" }
      ]
    },
    {
      "source": "/:path*\\.gif",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=2592000, immutable" }
      ]
    },
    {
      "source": "/:path*\\.webp",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=2592000, immutable" }
